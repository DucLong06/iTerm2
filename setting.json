{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnType": true,
        "diffEditor.ignoreTrimWhitespace": true,
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "http.proxy": "http://proxy.fpt.vn:80",
    "http.proxyStrictSSL": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.defaultColorDecorators": "auto",
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": "active",
    "editor.insertSpaces": true,
    "editor.linkedEditing": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.tabSize": 4,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.watcherExclude": {
        "/.ammonite": true,
        "/.bloop": true,
        "**/.metals": true
    },
    "git.autofetch": true,
    "git.confirmSync": true,
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.schemas": [],
    "prettier.tabWidth": 4,
    "prettier.useTabs": true,
    "python.languageServer": "Pylance",
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.indexing": false,
    "python.analysis.typeCheckingMode": "off",
    "python.formatting.blackPath": "autopep8",
    "python.formatting.provider": "none",
    "security.workspace.trust.untrustedFiles": "newWindow",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.inheritEnv": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.editor.splitInGroupLayout": "vertical",
    "workbench.editorAssociations": {
        "*.parquet": "default"
    },
    "workbench.iconTheme": "vscode-icons",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "redhat.telemetry.enabled": true,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "python.diagnostics.sourceMapsEnabled": true,
    "files.associations": {
        "*.py": "python",
        "*.tsx": "typescriptreact",
        "*.js": "javascript",
        "*.ts": "typescript",
    },
    "python.analysis.completeFunctionParens": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "workbench.colorTheme": "Cobalt2",
    // This is all that matters
    "editor.fontFamily": "JetBrains Mono",
    "editor.letterSpacing": 0.5,
    // Vim setting
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-o>"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "<C-p>"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "before": [
                "J"
            ],
            "after": [
                "5",
                "j",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "j",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "k",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "K"
            ],
            "after": [
                "5",
                "k",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-w>",
                "j"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-w>",
                "k"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        {
            "before": [
                "<Leader>",
                "p"
            ],
            "commands": [
                "workbench.action.quickOpen",
            ]
        },
        {
            "before": [
                "<Leader>",
                "t"
            ],
            "commands": [
                "workbench.action.gotoSymbol",
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "revealInExplorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "after": [
                "_"
            ]
        },
        {
            "before": [
                "<leader>",
                "l"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "o"
            ],
            "commands": [
                "workbench.action.showEditorsInActiveGroup"
            ]
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-i>": false,
        "<C-d>": false,
        "<C-c>": false,
        "<C-x>": false,
        "<C-v>": false,
        "<C-z>": false,
        "<C-y>": false
    },
    "vim.leader": "<Space>",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.sneak": true,
    "window.commandCenter": false,
    "files.autoSave": "afterDelay",
    "remote.autoForwardPortsSource": "hybrid",
    "vsicons.dontShowNewVersionMessage": true,
    "[dockercompose]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "autopep8.args": [
        "--max-line-length=120"
    ],
    "terminal.integrated.copyOnSelection": true,
    "[django-html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "gitlens.launchpad.indicator.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "python": true
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-linux": "/home/longhd/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.kubectl-path-linux": "/home/longhd/.local/state/vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path-linux": "/home/longhd/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "makefile.configureOnOpen": true,
    "debug.onTaskErrors": "abort",
    "explorer.confirmPasteNative": false,
    "search.showLineNumbers": true,
    "zenMode.hideLineNumbers": false,
    "vim.smartRelativeLine": false,
    "notebook.lineNumbers": "on",
    "window.customTitleBarVisibility": "auto",
    "[helm]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "redhat.vscode-yaml",
    },
    "database-client.autoSync": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "tabnine.experimentalAutoImports": true,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:o3-mini",
    "terminal.integrated.suggest.enabled": true,
}